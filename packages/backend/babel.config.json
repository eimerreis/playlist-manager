{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				}
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"dotenv-import",
			{
				"moduleName": "@env",
				"path": ".env",
				"blacklist": "null",
				"whitelist": "null",
				"safe": false,
				"allowUndefined": false
			}
		],
		"@babel/plugin-proposal-class-properties"
	]
}
