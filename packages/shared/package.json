{
  "name": "@eimerreis/playlist-manager-shared",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build-dependencies": "yarn workspace @eimerreis/playlist-manager-shared build",
